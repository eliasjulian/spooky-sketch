{
  "name": "spooky",
  "description": "Have you ever wondered if an image you're using is spooky enough? Well then boy, is this the plugin 4 u. Happy Halloween!",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Spooky",
    "manifest": "src/manifest.json",
    "main": "spooky.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Elias Julian <eliasneverlost@gmail.com>"
}
